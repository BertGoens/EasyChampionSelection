<Window
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    x:Name="wndMyClientOverload"
    x:Class="EasyChampionSelection.wndClientOverload"
    Height="50" Width="400" ShowInTaskbar="False" Topmost="True" WindowStyle="None" ShowActivated="False"
    IsVisibleChanged="wndMyClientOverload_IsVisibleChanged" ResizeMode="NoResize" Title="ECS lolClient Overlay">
    
    <Grid Background="#FF1D9393">
        <ComboBox x:Name="cboGroups" 
                  HorizontalAlignment="Left" Margin="75,9,0,0" VerticalAlignment="Top" Width="300" Height="25" UseLayoutRounding="False" MinWidth="300" MinHeight="25" />
        <Label Content="Groups:" HorizontalAlignment="Left" Margin="10,9,0,0" VerticalAlignment="Top" />
    </Grid>
</Window>
