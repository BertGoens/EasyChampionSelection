<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <LinearGradientBrush x:Key="windowMainBackground" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FF8FAEDD" Offset="0"/>
        <GradientStop Color="#FF6088C4" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="windowHelperBackground" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FF66A1C3" Offset="1"/>
        <GradientStop Color="#FF66B6C3"/>
    </LinearGradientBrush>
    
    <ControlTemplate x:Key="ThumbOldPosRectangleTemplate">
        <Rectangle Width="60" Height="30" Fill="White" Opacity="0.8" />
    </ControlTemplate>

    <ControlTemplate x:Key="ThumbNewPosRectangleTemplate">
        <Rectangle Width="100" Height="100" Fill="Black" Opacity="0.8" />
    </ControlTemplate>
</ResourceDictionary>